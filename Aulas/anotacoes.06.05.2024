VEICULO:
 CD_VEICULO
 DS_VEICULO

PEDIDO:
 ID_PEDIDO
 CD_PEDIDO
 CD_CLIENTE
 QT_ITEM
 DT_PEDIDO

CLIENTE:
 CD_CLIENTE
 DS_CLIENTE

 SELECT C.DS_CLIENTE, V.DS.VEICULO, P.ID_PEDIDO, P.QT_ITEM 
 FROM PEDIDO P, CLIENTE C, VEICULO V

 WHERE P.CD_CLIENTE = C.CD_CLIENTE AND P.CD_VEICULO=V.CD_VEICULO
 ORDER BY
 C.DS_CLIENTE
